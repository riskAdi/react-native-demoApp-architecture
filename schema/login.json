{
    "$id": "http://api/schemas/users/login.json#",
    "description": "login",
    "title": "login",
    "type": "object",
    "required": [
        "username",
        "password"
    ],
    "properties": {

        "username":{
            "type": "string",
            "minLength": 5,
            "maxLength": 70,
            "transform":["trim"]
        },
        "password":{
            "type": "string",
            "minLength": 5,
            "maxLength": 20,
            "transform":["trim"]
        }  
    }
}
